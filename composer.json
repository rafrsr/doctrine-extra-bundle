{
  "name": "rafrsr/doctrine-extra-bundle",
  "description": "A Symfony bundle to add some helpful functionalities to use with doctrine",
  "license": "MIT",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "Rafael SR",
      "email": "raf.rsr@gmail.com",
      "homepage": "https://github.com/rafrsr"
    }
  ],
  "autoload": {
    "psr-4": {
      "Rafrsr\\DoctrineExtraBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rafrsr\\DoctrineExtraBundle\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=5.4",
    "symfony/framework-bundle": "^2.6|^3.0",
    "symfony/finder": "^2.6|^3.0",
    "doctrine/annotations": "~1.2",
    "doctrine/cache": "~1.5",
    "doctrine/orm": "~2.3",
    "doctrine/doctrine-bundle": "~1.4",
    "rafrsr/crypto": "~2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.1|^5.1",
    "satooshi/php-coveralls": "dev-master"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
